#include <iostream>
#include <string>
using namespace std;

int main() {
    int numericalGrade;
    cout << "Enter your numerical grade:";
    cin >> numericalGrade;

    string letterGrade;
    if (numericalGrade >= 90) {
        letterGrade = "A";
    } else if (numericalGrade >= 80) {
        letterGrade = "B";
    } else if (numericalGrade >= 70) {
        letterGrade = "C";
    } else if (numericalGrade >= 60) {
        letterGrade = "D";
    } else {
        letterGrade = "F";
    }

    cout << "Your letter grade is: " << letterGrade << endl;

    return 0;
}
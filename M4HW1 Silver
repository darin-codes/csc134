#include <iostream>

int main() {
    int number;

    std::cout<<"Enter a number from 1 to 12? ";
    std::cin>> number;

    for (int i=1; i<=12; ++i) {
        int product = number * i;
        std::cout<<number<<"times"<<i<<"is"<<product<<"."<<std::endl;
    }

    return 0;

}